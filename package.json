{
  "name": "reactjs-scaffolding",
  "version": "1.0.0",
  "description": "Kickstarter project for ReactJS applications",
  "main": "dev-server.js",
  "scripts": {
    "lint": "eslint src/** --fix",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "NODE_ENV=development ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --port 8080 --host 0.0.0.0 --config webpack.config.js --progress",
    "start:fake-api": "NODE_ENV=development FAKE_API=true ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --port 8080 --host 0.0.0.0 --config webpack.config.js --progress",
    "build": "npm run eslint && rimraf dist && NODE_ENV=production webpack"
  },
  "author": "Damian Aruj <damian.aruj@whiteprompt.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.22.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "css-loader": "0.26.1",
    "eslint": "3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.10.0",
    "html-webpack-plugin": "2.28.0",
    "image-webpack-loader": "3.2.0",
    "node-sass": "4.5.0",
    "open-browser-webpack-plugin": "0.0.3",
    "postcss-loader": "1.3.1",
    "pre-commit": "1.2.2",
    "rimraf": "2.5.4",
    "sass-loader": "6.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack-server": "0.1.2",
    "webpack-vendor-chunk-plugin": "1.0.0"
  },
  "dependencies": {
    "axios": "0.15.3",
    "babel-polyfill": "6.23.0",
    "eslint": "3.15.0",
    "lodash-es": "4.17.4",
