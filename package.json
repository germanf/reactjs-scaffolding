    "react-redux": "5.0.2",
    "react-router-dom": "4.0.0",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-logger": "2.8.1",
    "redux-promise-middleware": "^4.2.0",
    "redux-reset": "^0.3.0",
    "redux-thunk": "2.2.0",
    "validator": "^7.0.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.1.0-beta.9"
  }
}
